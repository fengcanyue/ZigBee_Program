    240          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    241          {
    242          
    243            osal_event_hdr_t  *msg_ptr;
    244            uint16 length = 0;
    245            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    246          
    247            /*
    248               If maxZAppBufferLength is 0 or larger than current length
    249               the entire length of the current buffer is returned.
    250            */
    251            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    252            {
    253              length = MT_UartMaxZAppBufLen;
    254            }
    255            else
    256            {
    257              length = rxBufLen;
    258            }
    259          
    260            /* Verify events */
    261            if (event == HAL_UART_TX_FULL)
    262            {
    263              // Do something when TX if full
    264              return;
    265            }
    266          
    267            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    268            {
    269              if ( App_TaskID )
    270              {
    271                /*
    272                   If Application is ready to receive and there is something
    273                   in the Rx buffer then send it up
    274                */
    275                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    276                {
    277                  /* Disable App flow control until it processes the current data */
    278                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    279          
    280                  /* 2 more bytes are added, 1 for CMD type, other for length */
    281                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    282                  if ( msg_ptr )
    283                  {
    284                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    285                    msg_ptr->status = length;
    286          
    287                    /* Read the data of Rx buffer */
    288                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    289          
    290                    /* Send the raw data to application...or where ever */
    291                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    292                  }
    293                }
    294              }
    295            }
